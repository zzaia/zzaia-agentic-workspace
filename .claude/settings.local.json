{
  "permissions": {
    "allow": [
      "Bash(git clone:*)",
      "Bash(git worktree:*)",
      "Bash(mkdir:*)",
      "Bash(git symbolic-ref:*)",
      "Bash(tree:*)",
      "Bash(python3:*)",
      "SlashCommand(/websearch deep learning classification models 2025)",
      "SlashCommand(/websearch:*)",
      "Bash(git --git-dir=.git --work-tree=. config core.bare false)",
      "Bash(git --git-dir=.git remote:*)",
      "Bash(git --git-dir=.git branch:*)"
    ]
  },
  "outputStyle": "agentic-concise",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$CLAUDE_PROJECT_DIR/.claude/hooks/SessionStart.sh"
          }
        ]
      }
    ]
  }
}
