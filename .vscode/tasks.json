{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "apphost:build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/Zzaia.AppHost/Zzaia.AppHost.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "apphost:clean",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "${workspaceFolder}/Zzaia.AppHost/Zzaia.AppHost.csproj"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
